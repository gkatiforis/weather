<?xml version="1.0" encoding="UTF-8"?>
<ehcache>
    <defaultCache eternal="true" maxElementsInMemory="0" overflowToDisk="true" />
    <cache name="temperaturePerStation"
           maxElementsInMemory="60000"
           timeToLiveSeconds="300"
           overflowToDisk="true"
           memoryStoreEvictionPolicy="LFU" />

    <cache name="weatherObservations"
           maxElementsInMemory="60000"
           timeToLiveSeconds="300"
           overflowToDisk="true"
           memoryStoreEvictionPolicy="LFU" />

    <cache name="countries"
           maxElementsInMemory="10000"
           timeToLiveSeconds="0"
           overflowToDisk="true"/>
</ehcache>